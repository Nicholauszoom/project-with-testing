{# {% extends 'base.html.twig' %}

{% block title %}Progress index{% endblock %}

{% block body %}
    <h1>Progress index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Description</th>
                <th>ImagePath</th>
                <th>CreatedAt</th>
                <th>UpdatedAt</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for progress in progress %}
            <tr>
                <td>{{ progress.id }}</td>
                <td>{{ progress.description }}</td>
                <td>{{ progress.imagePath }}</td>
                <td>{{ progress.createdAt ? progress.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ progress.updatedAt ? progress.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_progress_show', {'id': progress.id}) }}">show</a>
                    <a href="{{ path('app_progress_edit', {'id': progress.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_progress_new') }}">Create new</a>
{% endblock %}

 #}


